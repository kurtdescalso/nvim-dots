{
	"languageserver": {
		"go": {
			"command": "gopls",
			"rootPatterns": ["go.mod"],
			"trace.server": "verbose",
			"filetypes": ["go"]
		}
	},
	"coc.preferences.formatOnSaveFiletypes": [
		"css",
		"go",
		"javascript",
		"json"
	],
	"prettier.tabWidth": 4,
	"prettier.useTabs": true,
	"prettier.disableSuccessMessage": true
}
